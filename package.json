{
  "name": "fp-ts-dom",
  "version": "0.1.0",
  "license": "MIT",
  "files": [
    "./lib",
    "./mjs",
    "./cjs",
    "./types"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./lib/index.mjs",
      "types": "./lib/index.d.mts"
    },
    "./*": {
      "import": "./lib/*.mjs",
      "types": "./lib/*.d.mts"
    }
  },
  "scripts": {
    "lint:eslint": "eslint --fix src",
    "lint:prettier": "prettier --write --loglevel=warn src",
    "lint": "npm-run-all lint:eslint lint:prettier",
    "build": "npm-run-all build:clean build:tsc",
    "build:clean": "rimraf ./lib tsconfig.tsbuildinfo",
    "build:tsc": "tsc",
    "dcd": "ts-prune",
    "madge": "madge --extensions mjs lib -c -s"
  },
  "peerDependencies": {
    "fp-ts": "^2.13.0-rc.6"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/prettier": "^2.7.0",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fp-ts": "^2.13.0-rc.6",
    "madge": "^5.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-prune": "^0.10.3",
    "typescript": "^4.8.3"
  },
  "packageManager": "yarn@3.2.3"
}
