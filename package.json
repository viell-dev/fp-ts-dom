{
  "name": "fp-ts-dom",
  "version": "0.1.0",
  "type": "module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./mjs/index.mjs",
      "require": "./cjs/index.cjs",
      "types": "./types/index.d.mts"
    },
    "./*": {
      "import": "./mjs/*.mjs",
      "require": "./cjs/*.cjs",
      "types": "./types/*.d.mts"
    }
  },
  "main": "cjs/index.cjs",
  "module": "mjs/index.mjs",
  "types": "types/index.d.mts",
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "lint:eslint": "eslint --fix src",
    "lint:prettier": "prettier --write --loglevel=warn src",
    "lint": "npm-run-all lint:eslint lint:prettier",
    "build": "npm-run-all build:clean build:tsc build:babel",
    "build:clean": "rimraf ./mjs ./cjs ./types",
    "build:tsc": "tsc --build --force",
    "build:babel": "babel ./mjs -d ./cjs --out-file-extension .cjs"
  },
  "peerDependencies": {
    "fp-ts": "^2.13.0-rc.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.1",
    "@babel/plugin-transform-modules-commonjs": "^7.18.6",
    "@types/babel__core": "^7",
    "@types/eslint": "^8.4.6",
    "@types/prettier": "^2.7.0",
    "@types/rimraf": "^3",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "fp-ts": "^2.13.0-rc.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.3"
  }
}
